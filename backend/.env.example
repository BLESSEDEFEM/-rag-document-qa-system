# Database (Replace with your actual credentials)
DATABASE_URL=postgresql://USER:PASSWORD@HOST:5432/DB_NAME

# AI Services (Replace with your actual API keys)
PINECONE_API_KEY=your_pinecone_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
COHERE_API_KEY=your_cohere_api_key_here
VIRUSTOTAL_API_KEY=your_virustotal_api_key_here

# Virus Scanning (Set to 'true' to require virus scanning, 'false' to make it optional)
VIRUS_SCAN_REQUIRED=false

# Redis Cache (Optional - local Redis doesn't need password)
REDIS_HOST=localhost
REDIS_PORT=6379
# REDIS_PASSWORD=your_redis_password_here
REDIS_USE_SSL=false

# Authentication (Replace with your Clerk details)
CLERK_FRONTEND_API=https://your-instance.clerk.accounts.dev
CLERK_JWKS_URL=https://your-instance.clerk.accounts.dev/.well-known/jwks.json

# Environment
ENVIRONMENT=development